<tr>
  <th scope="row"><%= user.id %></th>
  <td><%= user.username %></td>
  <td><%= user.usersurname %></td>
  <td><%= user.email %></td>
  <td><%= user.role %></td>
  <td><%= l(user.created_at) %></td>
  <td>
    <%= link_to t('actions.edit'), edit_admin_user_path(user), class: 'btn btn-secondary' %>
    <%= link_to t('actions.delete'), admin_user_path(user), class: 'btn btn-danger', data: { turbo_confirm: t('actions.confirmation'), turbo_method: :delete } %>
  </td>
</tr>
